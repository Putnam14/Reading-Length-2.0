{
  "name": "reading-length-backend",
  "version": "1.1.0",
  "description": "Yoga server for Reading Length",
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js",
    "dev": "nodemon -e js,graphql -x node --inspect src/index.js",
    "test": "jest",
    "deploy-dev": "prisma deploy --env-file variables.env",
    "deploy-staging": "prisma deploy --env-file staging.variables.env",
    "deploy-prod": "prisma deploy --env-file prod.variables.env"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "dotenv": "^6.2.0",
    "graphql": "^14.0.2",
    "graphql-cli": "^3.0.4",
    "graphql-yoga": "^1.16.9",
    "nodemon": "^1.18.9",
    "piranhax": "^1.0.9",
    "prisma": "^1.24.0",
    "prisma-binding": "^2.2.14",
    "waait": "^1.0.4"
  },
  "repository": "https://github.com/Putnam14/Reading-Length-2.0",
  "author": "Bridger Putnam",
  "license": "MIT",
  "babel": {
    "presets": [
      "env"
    ]
  }
}
