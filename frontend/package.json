{
  "name": "reading-length-frontend",
  "version": "1.0.0",
  "description": "Front end for Reading Length",
  "main": "index.js",
  "repository": "https://github.com/Putnam14/Reading-Length-2.0",
  "author": "Bridger Putnam",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development yarn next build && yarn run start",
    "build": "next build",
    "now-build": "next build",
    "start": "NODE_ENV=development node server.js",
    "test": "NODE_ENV=test jest --watch",
    "deploy-staging": "now -A staging.now.json && now alias -A staging.now.json"
  },
  "dependencies": {
    "@babel/core": "^7.3.3",
    "apollo-boost": "^0.1.28",
    "apollo-client": "^2.4.13",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "downshift": "^3.2.2",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "isbot": "^2.2.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "next": "^8.0.1",
    "next-with-apollo": "^3.3.2",
    "nprogress": "^0.2.0",
    "path-match": "^1.2.4",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.8.2",
    "react-headroom": "2.2.2",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.14.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "graphql-tools": "^4.0.4",
    "jest": "^23.6.0",
    "jest-transform-graphql": "^2.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest",
      "^.+\\.js?$": "babel-jest"
    }
  }
}
